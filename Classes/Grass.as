package {	import flash.display.MovieClip;	import flash.events.Event;	public class Grass extends MovieClip 	{		public function Grass() 		{			scaleX = 2;			scaleY = 2;			y = 293.5;			addEventListener(Event.ADDED_TO_STAGE, init);		}				public function init(evt:Event){			removeEventListener(Event.ADDED_TO_STAGE, init);			x = stage.stageWidth/2;			y = stage.stageHeight-this.height/2;		}		}}